{
  "name": "scriptnotebook",
  "displayName": "script noteBook",
  "publisher": "awsxxf",
  "version": "0.0.3",
  "description": "vscode generic notebook surport with integrated terminal",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AWSXXF/vscode-serial-terminal.git"
  },
  "homepage": "https://github.com/AWSXXF/vscode-serial-terminal/blob/main/scriptNoteBook/README.md",
  "bugs": {
    "url": "https://github.com/AWSXXF/vscode-serial-terminal/issues"
  },
  "keywords": [
    "script notebook",
    "terminal notebook"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "notebooks": [
      {
        "type": "scrnb",
        "displayName": "Script Notebook",
        "selector": [
          {
            "filenamePattern": "*.scrnb"
          }
        ]
      }
    ],
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}