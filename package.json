{
  "name": "serialterminal",
  "displayName": "serial terminal",
  "publisher": "awsxxf",
  "description": "An interactive serial terminal tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/AWSXXF/vscode-serial-terminal.git"
  },
  "bugs": {
    "url": "https://github.com/AWSXXF/vscode-serial-terminal/issues"
  },
  "icon": "./assets/logo.png",
  "l10n": "./l10n",
  "pricing": "Free",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "entryView",
          "title": "%entryView.title%",
          "icon": "$(debug-disconnect)"
        }
      ]
    },
    "views": {
      "entryView": [
        {
          "name": "%TreeView.serialPort%",
          "id": "serialport.serialportView"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "serialTerminal.openSerialPort",
          "when": "view == serialport.serialportView",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "serialTerminal.openSerialPort",
        "icon": "$(arrow-right)",
        "title": "%commands.openSerialTerminal%"
      }
    ]
  },
  "keywords": [
    "serial port",
    "serial",
    "com",
    "terminal",
    "console",
    "uart"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/l10n-dev": "^0.0.26",
    "@vscode/test-electron": "^2.3.0",
    "electron": "^25.0.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.14",
    "colors": "^1.4.0",
    "node-gyp": "^9.3.1",
    "serialport": "^11.0.0"
  }
}